  --corH3: var(--roxoGoogle);
  --corH5: var(--verde-servo);
  --corNegrito: black; 
  --corHoverBotao: navajowhite ;
  --switch-shadow: -2px 1px 10px #ffffff;
  --fundo-carousel: none;
  --corTextoCode: #eee ;
  --strong-color: black;
  --texto-dim: darkgray ;
  --letragradiente: black;
  --katex-color: var(--corFonte);
  /*
  --corBotao: var(--fundo-input);
  */
  --corFundoHoverBotao: #2e2c35;
  --headings: black;
  --corBordas: #282828;
  --fundo-invertido: darkgray ;
  --fundo-input: tan;
  --separador: darkgray;
  --corFonte: #282828;
  --corFonteInvertida: white;
  --frase: var(--azul-gemini);
  --fraseShow: black;
  --fraseHover: darkred ;
  --sticker-fundo: var(--gradiente-light) ;
  --corLinks: var(--azul-gemini) ;
  --corLinksVisitados: var(--azul-escuro-google) ;
  --corLinksHover: black;
  --corTermo: var(--pimenta);
  --link-barra: var(--tomato-gemini);
  --fundo: #f4f4f4; 
  --gradfundo: linear-gradient(
                to bottom, 
                lightblue,
                #eee
               );
  --sombra: #353535;
}
