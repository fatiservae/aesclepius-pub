@font-face {
    font-family: 'comic sans';
    src: url('/fonts/comicsans.woff2') format('woff2'), /* Formato de fonte WOFF2 */
         url('/fonts/comicsans.ttf') format('ttf'); /* Formato de fonte WOFF */
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'virgil';
    src: url('/fonts/Virgil.woff2') format('woff2'), /* Formato de fonte WOFF2 */
    font-weight: normal;
    font-style: normal;
}

:root {
  /* CORES */
  --corDailyDevGradiente: linear-gradient(
                            90deg,
                            #fc538e,
                            #d741df,
                            );
  --corGradienteGemini: linear-gradient(
                      90deg, 
                      #4b83f0, 
                      #6b7ce1, 
                      #8776d4, 
                      #ab6fb4, 
                      #be6b98, 
                      #d96570, 
                      #d56676);
  --corGradienteGray: linear-gradient(
                      90deg, 
                      var(--corFonte),
                      #777,
                      #666,
                      #444 );

  --azulpapel: #c3e4e5;
  --azulbreve: #0960ee;
  --lime: #ffff00;
  --ferrugem1: #92340b;
  --ferrugem2: #6d2708;
  --ferrugem3: #491a05;
  --cafe: #744a31;
  --mocha: #e9dbb5;
  --palido: #FFECB3; 
  --azulJornal: #0256a1;
  --laranjaJornal: #fbb013;
  --rosaJornal: #ff6d91;
  --musgo: #347d39 ;
  --azulCalcinha: #539bf5 ;
  --pimenta: #bb1450 ;
  --vermelhoMITMarcador: #e56478;
  --branco-google: #f2f2f2 ;
  --amarelo-google: #ffa700 ;
  --azul-gemini: #4b83f0;
  --roxo-gemini: #8776d4; 
  --rosa-gemini: #be6b98;
  --tomato-gemini: #d56676;
  --madeira: #453b3a;
  --azul-google: #0057e7 ;
  --azul-escuro-google: #3f51b5 ;
  --vermelho-google: #d62d20 ;
  --azul-estadao: rgb(37, 117, 232);
  --verde-google: #008744 ;
  --amarelo-servo: #faae30;
  --laranja-servo: #f68243;
  --lilas-servo: #f03278;
  --laranja: #ff4717;
  --laranja2: #ff8503 ;
  --verde-servo: #4fc066;
  --roxoGoogle: #673ab7 ;
  --abobora-google: #f44336 ;
  --solarized: #eee8d5 ;
  --anil: #283784 ;
  --corBordas: #383f48 ;
  --fundo: #22272e ;
  --separador: #383f48 ;
  --corFonte: #a0acb9 ;
  --git-hub-dark-back: #22272e ;
  --azul-vibrante: #01bfff ;
  --creme: #f5f5dc ;
  --vermelhoDailyBeast: #ea0606;

  /* CORES TRANSPARENTE */
  --amarelo-google-transp: #ffa70010 ;
  --git-hub-roxo-escuro-transp: #bf398910 ;
  --vidro: rgba(8, 8, 7, .15);

  /* CORES GRADIENTES */ 
  --gradienteCafe: linear-gradient(
                    90deg, 
                    #5b3027, 
                    var(--ferrugem3), 
                    var(--madeira)
                   );
  --left-gradiente-dark: var(radial-gradient(
                                ellipse at 50% 50%, 
                                #bf398910 0, 
                                transparent 75%), 
                             radial-gradient(
                                ellipse at 50% 50%, 
                                #bf398910 0, 
                                transparent 75%)) ;
  --left-gradiente-light: var(radial-gradient(
                                ellipse at 50% 50%, 
                                #c61d1020 0, 
                                transparent 75%), 
                              radial-gradient(
                                ellipse at 50% 50%, 
                                #ffa70050 0, 
                                transparent 75%)) ;
  --letragradiente-light: linear-gradient(
                              45deg, 
                              black, 
                              darkred, var(--corFonte));
  --gradiente-dark: radial-gradient(
                      ellipse at 50% 50%, 
                      #bf398910 0, 
                      transparent 75%), 
                    radial-gradient(
                      ellipse at 50% 50%, 
                      #bf398910 0, 
                      transparent 75%); 
  --gradienteYoutube: linear-gradient(
                        to right,
                        #ff8983 17.85%,
                        #7f0e7f 53.28%,
                        #007a65 100%);
  --gradiente-light: radial-gradient(
                       ellipse at 50% 50%, 
                       #c61d1020 0, 
                       transparent 55%), 
                     radial-gradient(
                       ellipse at 50% 50%, 
                       #ffa70050 0, 
                       transparent 75%);
  --gradiente-sidekick: linear-gradient(
                          38.91deg, 
                          rgb(255, 61, 61) 39.17%, 
                          rgb(89, 86, 255) 79.55%);

  /* PROPRIEDADES */
  --iconeBarraSize: 30px;
  scrollbar-color: var(--barraRolagem) var(--fundo);
  /* --margem: 5vw; */
  --margem2: 7vw;
  --margem3: 9vw;
  --margem4: 11vw;
  --margem5: 13vw;
  --corH4: var(--negrito);
  --h4BeforeContent: "-- ";
  --bordas: 1px solid var(--corFonte);
  --corFonteBarraHover: var(--corNegrito);
  --temaFonte: "IBM Plex Sans" ;
  --displayFigurasFundo: block;
  --bordasButao: 8px;
  --fundoSticker: var(--gradiente-dark);
  --raioCampos: 34px; 
  --sombraCaixas: none;
  --sombraImagens: none;
  --raioBordas: 10px;
  /* Quanto maior o componente vw, melhor em monitores
   * grandes para aproveitamento de tela e pior em mon
   * itores menores. E vice-versa.
   * O terceiro componente é um cálculo que privilegia
   * monitores grandes para compensar.
  */
  --largura-texto: calc(650px + 10vw + calc(calc(100vw - 600px) * 30/100));
  --clipHeading: text; 
  --corFundoSelecao: tomato;
  --corSelecao: black;
  --h3BeforeContent: "•  ";
  /* Necessário pra manter os 
   * SVG com var(--fonte) 
   * compatíveis com --corFonte 
  */
  --fonte: var(--corFonte); 
}

::selection {
    background-color: var(--corFundoSelecao); 
    color: var(--corSelecao);
}

:root[data-tema="light"] { 
  /*
  --corH2: var(--letragradiente-dark);
  */
