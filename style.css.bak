title {
}

html {
  font-family: var(--temaFonte);
}

body {
  /*
  background-image: var(--fundoImagem);
  padding: 0%;
  margin-left: auto;
  margin-right: auto;
  max-width: 80ch;
  */
  padding: 2ch;
  background: var(--fundo);
  background-attachment: fixed;
  color: var(--corFonte) ;
  scroll-behavior: smooth ;
  font-size: clamp(16px, calc(2vw - 5px), 24px);
}
@media only screen and (max-width: 800px){
  body{
    font-size: 105%;
    background-image: none;
  }
}

@keyframes blinkEmConstrucao {
  0% { color: black; background: red}
  50% { color: pink; background: #282828}
  100% { color: red; background: black}
}
.emConstrucao {
  font-weight: bold;
  width: fit-content;
  padding: 1rem;
  animation: blinkEmConstrucao 2s infinite;
}

#switch {
  /*
  position: absolute;
  top: 1vw;
  font-size: 200%;
  left: 5vw;
  text-shadow: var(--switch-shadow);
  */
  border: none;
}
a#switch {
  /*
  transition: box-shadow 200ms ease-in-out;
  margin-right: 0.5em;
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.5), 0 1px 5px rgba(0, 0, 0, 0.5);
  font-size: 150%;
  box-shadow: none;
