}

.sticker {
  /*
  border: 1px solid var(--corBordas);
  */
  margin-left: var(--margem);
  background: var(--sticker-fundo);
  border-radius: 5px;
  text-align: start;
  width: fit-content;
  font-weight: bold ;
  padding: 0.5em;
  max-width: 80%;
  overflow: hidden; 
  color: var(--sticker-font); 
}
.etiqueta {
  /*
  */
  border: var(--bordaEtiqueta);
  overflow: wrap;
  padding: 0.5em;
  margin-right: auto;
  max-width: var(--largura-texto);
  background: var(--sticker-fundo);
  border-radius: var(--raioBordas);
  text-align: center;
  overflow: hidden; 
  color: var(--sticker-font); 
  font-weight: normal;
  transition: color 4s ease-in-out, font-weight 2s ease-in-out;
}
.etiqueta::before,
.etiqueta::after {
  /*
  content: ' "';
  font-size: 200%;
  */
}
.etiqueta:hover{
  color: var(--strong-color);
}
