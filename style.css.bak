  --corBarraHover: white;
  --corListas: red; 
  --bordaEtiqueta: 1px solid darkgray;
  --bordas: 1px solid var(--corBordas);
  --corMark: black;
  --corProgressBar: tomato;
  --imageBackgroundHeadings: var(--corH2-gradiente); 
  --bordasHeading: none;
  --fundoHeading: none;
  --corFundoCode: #303030;
  --barraRolagem: #282828;
  --highlightCor: tomato;
  --corBarraSelecionado: var(--amarelo-servo);
  --fundo-card: linear-gradient(85deg,#101010,#262626);
  --corH2: darkred;
  --corH4: var(--azul-estadao);
  /*
  --corH2: transparent;
  --corH2-gradiente: linear-gradient(
                      90deg, 
                      #4b83f0, 
                      #6b7ce1, 
                      #8776d4, 
                      #ab6fb4, 
                      #be6b98, 
                      #d96570, 
                      #d56676
                     );
  */
