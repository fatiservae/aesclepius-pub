/*CLASSES DE ALINHAMENTO*/
.importante {
  background-color: #983500;
  font-weight: normal;
  border-radius: 3px;
  font-weight: bold;
  font-style: italic;
  text-align: center;
  color: black;
  text-indent: 0%;
