  text-align: center ;
  margin-left: auto;
  margin-right: auto;
}
.left {
  padding-left: 0px;
  margin-left: var(--margem);
  align: left;
  text-align: left;
}
.right{
  margin: auto;
  align: right;
  text-align: right ;
}
.primeiraMaiuscula::first-letter {
  text-transform: uppercase;
}

[id^="resultado"] {
  margin-left: 0;
